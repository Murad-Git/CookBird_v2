            <div class="container">
                <!-- contener to margin content -->
                <div class="container-in">
                    <div class="row">
                        <!-- SECTION: SEARCH SIDE BAR -->
                        <section class="col-md-3 cookbird-side-bar">
                            <div class="filters">
                                <div class="accordion" id="filter-ingredients">
                                    <div class="accordion-group">
                                        <h2 data-toggle="filter-group">
                                            Main Ingredient
                                            <button class="toggler expand"><i class="fas fa-chevron-circle-down"></i></button>
                                        </h2>
                                        <div class="accordion-body coppalse in" id="collapseOne">
                                            <div class="accordion-inner">
                                                <ul >
                                                    <li >first item</li>
                                                    <li >second item</li>
                                                    <li >third item</li>
                                                    <li >fourth item</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion" id="filter-cuisine">
                                    <div class="accordion-group">
                                        <h2 data-toggle="filter-group">
                                            Main Ingredient
                                            <button class="toggler expand"><i class="fas fa-chevron-circle-down"></i></button>
                                        </h2>
                                        <div class="accordion-body coppalse in" id="collapseTwo">
                                            <div class="accordion-inner">
                                                <ul >
                                                    <li >first item</li>
                                                    <li >second item</li>
                                                    <li >third item</li>
                                                    <li >fourth item</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion" id="filter-others">
                                    <div class="accordion-group">
                                        <h2 data-toggle="filter-group">
                                            Main Ingredient
                                            <button class="toggler expand"><i class="fas fa-chevron-circle-down"></i></button>
                                        </h2>
                                        <div class="accordion-body coppalse in" id="collapseThird">
                                            <div class="accordion-inner">
                                                <ul >
                                                    <li >first item</li>
                                                    <li >second item</li>
                                                    <li >third item</li>
                                                    <li >fourth item</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <!-- SECTION: RECIPES CONTENT -->
                        <section class="col-md-9" id="cookbird-thumbs">
                            <div class="row cookbird-thumbs-out">

                                <%recipes.forEach(recipe=>{%>
                                    <div class="recipe-thumb col-md-4">
                                        <a class="recipe-anchor" href="/<%=recipe.id%>">
                                            <div class="recipe-img-link">
                                                <figure>
                                                    <img src="<%=recipe.image%>">
                                                </figure>
                                            </div>
                                            <h3><%=recipe.title%></h3>
                                        </a>
                                    </div>
                                <%})%>

                                <!-- <div class="recipe-thumb col-md-4">
