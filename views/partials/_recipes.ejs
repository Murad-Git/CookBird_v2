<%recipes.forEach(recipe=>{%>
    <div class="recipe-thumb col-lg-4 col-md-6 col-xs-12">
        <a class="recipe-anchor" href="/<%=recipe.id%>">
            <div class="recipe-anchor__link">
                <figure>
                    <img class="recipe-main-img" src="<%=recipe.image%>" alt="<%=recipe.title%>">
                </figure>
            </div>
            <h3><%=recipe.title%></h3>
        </a>
    </div>
<%})%>